{
    "name": "brunocfalcao/eduka-nereus",
    "description": "Eduka nereus package - multi site context awareness",
    "keywords": [
        "brunocfalcao",
        "laravel",
        "eduka"
    ],
    "homepage": "https://github.com/brunocfalcao/eduka-nereus",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bruno Falc√£o",
            "email": "bruno.c.falcao@brunofalcao.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "brunocfalcao/laravel-helpers": "dev-master",
        "brunocfalcao/eduka-dev": "dev-master",
        "brunocfalcao/cerebrus": "dev-master",
        "brunocfalcao/eduka-cube": "dev-master",
        "brunocfalcao/eduka-payments": "dev-master",
        "brunocfalcao/eduka-abstracts": "dev-master",
        "brunocfalcao/eduka-services": "dev-master",
        "brunocfalcao/eduka-migrations": "dev-master",
        "brunocfalcao/eduka-nova": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Eduka\\Nereus\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eduka\\Nereus\\NereusServiceProvider"
            ],
            "aliases": {
                "Nereus": "Eduka\\Nereus\\Facades\\Nereus"
            }
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true
}
